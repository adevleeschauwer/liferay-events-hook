<?xml version="1.0"?>


<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	     version="2.0">
    <portlet>
	<portlet-name>Contacts-portlet</portlet-name>
	<display-name>Contacts Portlet</display-name>
	<portlet-class>com.liferay.events.global.mobile.portlet.ContactsPortlet</portlet-class>
	<init-param>
	    <name>view-template</name>
	    <value>/html/contacts/view.jsp</value>
	</init-param>
	<expiration-cache>0</expiration-cache>
	<supports>
	    <mime-type>text/html</mime-type>
	</supports>
	<portlet-info>
	    <title>Contacts-portlet</title>
	    <short-title>Contacts-portlet</short-title>
	    <keywords>Contacts-portlet</keywords>
	</portlet-info>
	<security-role-ref>
	    <role-name>administrator</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>guest</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>power-user</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>user</role-name>
	</security-role-ref>
    </portlet>

    <portlet>
	<portlet-name>Messages-portlet</portlet-name>
	<display-name>Messages Portlet</display-name>
	<portlet-class>com.liferay.events.global.mobile.portlet.MessagesPortlet</portlet-class>
	<init-param>
	    <name>view-template</name>
	    <value>/html/messages/view.jsp</value>
	</init-param>
	<expiration-cache>0</expiration-cache>
	<supports>
	    <mime-type>text/html</mime-type>
	</supports>
	<portlet-info>
	    <title>Messages-portlet</title>
	    <short-title>Messages-portlet</short-title>
	    <keywords>Messages-portlet</keywords>
	</portlet-info>
	<security-role-ref>
	    <role-name>administrator</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>guest</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>power-user</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>user</role-name>
	</security-role-ref>
    </portlet>
    <portlet>
	<portlet-name>Polls-portlet</portlet-name>
	<display-name>Polls Portlet</display-name>
	<portlet-class>com.liferay.events.global.mobile.portlet.PollsPortlet</portlet-class>
	<init-param>
	    <name>view-template</name>
	    <value>/html/polls/view.jsp</value>
	</init-param>
	<expiration-cache>0</expiration-cache>
	<supports>
	    <mime-type>text/html</mime-type>
	</supports>
	<portlet-info>
	    <title>Polls-portlet</title>
	    <short-title>Polls-portlet</short-title>
	    <keywords>Polls-portlet</keywords>
	</portlet-info>
	<security-role-ref>
	    <role-name>administrator</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>guest</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>power-user</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>user</role-name>
	</security-role-ref>
    </portlet>
    <portlet>
	<portlet-name>PollsResults-portlet</portlet-name>
	<display-name>PollsResults Portlet</display-name>
	<portlet-class>com.liferay.util.bridges.mvc.MVCPortlet</portlet-class>
	<init-param>
	    <name>view-template</name>
	    <value>/html/polls/results.jsp</value>
	</init-param>
	<init-param>
	    <name>config-template</name>
	    <value>/html/polls/polls-results-configuration.jsp</value>
	</init-param>
	<expiration-cache>0</expiration-cache>
	<supports>
	    <mime-type>text/html</mime-type>
	</supports>
	<portlet-info>
	    <title>PollsResults-portlet</title>
	    <short-title>PollsResults-portlet</short-title>
	    <keywords>PollsResults-portlet</keywords>
	</portlet-info>
	<security-role-ref>
	    <role-name>administrator</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>guest</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>power-user</role-name>
	</security-role-ref>
	<security-role-ref>
	    <role-name>user</role-name>
	</security-role-ref>
    </portlet>


</portlet-app>